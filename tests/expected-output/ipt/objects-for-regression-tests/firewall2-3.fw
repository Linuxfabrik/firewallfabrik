 $IPTABLES -w -N RULE_11
 $IPTABLES -w -A INPUT -p icmp -m icmp -s 192.168.2.0/24 -d 192.168.2.1 -m conntrack --ctstate NEW -j RULE_11
 $IPTABLES -w -A RULE_11 -j LOG --log-level debug --log-prefix "RULE 11 - ACCEPT **"
 $IPTABLES -w -A RULE_11 -j ACCEPT
 #
 # Rule 12 (global)
 #
 echo "Rule 12 (global)"
 #
 $IPTABLES -w -N CHAIN
 $IPTABLES -w -A FORWARD -d 211.11.11.11 -m conntrack --ctstate NEW -j CHAIN
 $IPTABLES -w -A CHAIN -s 192.168.1.10 -j ACCEPT
 $IPTABLES -w -A CHAIN -s 192.168.1.20 -j ACCEPT
 #
 # Rule 13 (global)
 #
 echo "Rule 13 (global)"
 #
 $IPTABLES -w -N CHAIN
 $IPTABLES -w -A FORWARD -s 211.11.11.11 -m conntrack --ctstate NEW -j CHAIN
 $IPTABLES -w -A CHAIN -d 192.168.1.10 -j ACCEPT
 $IPTABLES -w -A CHAIN -d 192.168.1.20 -j ACCEPT
 #
 # Rule 14 (global)
 #
 echo "Rule 14 (global)"
 #
 $IPTABLES -w -N CHAIN
 $IPTABLES -w -A OUTPUT -j CHAIN
 $IPTABLES -w -A INPUT -j CHAIN
 $IPTABLES -w -A FORWARD -j CHAIN
 $IPTABLES -w -A CHAIN -s 192.168.1.0/24 -j RETURN
 $IPTABLES -w -A CHAIN -s 192.168.2.0/24 -j RETURN
 $IPTABLES -w -A CHAIN -j REJECT --reject-with icmp-net-unreachable
 #
 # Rule 15 (global)
 #
 echo "Rule 15 (global)"
 #
 $IPTABLES -w -A INPUT -s 192.168.1.0/24 -m conntrack --ctstate NEW -j ACCEPT
 $IPTABLES -w -A INPUT -s 192.168.2.0/24 -m conntrack --ctstate NEW -j ACCEPT
 $IPTABLES -w -A OUTPUT -s 192.168.1.0/24 -m conntrack --ctstate NEW -j ACCEPT
 $IPTABLES -w -A OUTPUT -s 192.168.2.0/24 -m conntrack --ctstate NEW -j ACCEPT
 $IPTABLES -w -A FORWARD -s 192.168.1.0/24 -m conntrack --ctstate NEW -j ACCEPT
 $IPTABLES -w -A FORWARD -s 192.168.2.0/24 -m conntrack --ctstate NEW -j ACCEPT
 #
 # Rule 16 (global)
 #
 echo "Rule 16 (global)"
 #
 $IPTABLES -w -N RULE_16
 $IPTABLES -w -A INPUT -s 192.168.1.0/24 -j RULE_16
 $IPTABLES -w -A OUTPUT -s 192.168.1.0/24 -j RULE_16
 $IPTABLES -w -A FORWARD -s 192.168.1.0/24 -j RULE_16
 $IPTABLES -w -A RULE_16 -j LOG --log-level debug --log-prefix "RULE 16 - DENY **"
 $IPTABLES -w -A RULE_16 -j DROP
 #
 # Rule 17 (global)
 #
 echo "Rule 17 (global)"
 #
 # host-fw2 has the same address as
 # one of the firewall's interfaces
 $IPTABLES -w -N RULE_17
 $IPTABLES -w -A OUTPUT -p tcp -m tcp -d 22.22.22.22 --dport 21 -m conntrack --ctstate NEW -m limit --limit 5/minute --limit-burst 10 -j RULE_17
 $IPTABLES -w -A FORWARD -p tcp -m tcp -d 22.22.22.22 --dport 21 -m conntrack --ctstate NEW -m limit --limit 5/minute --limit-burst 10 -j RULE_17
 $IPTABLES -w -A RULE_17 -j LOG --log-level debug --log-prefix "RULE 17 - ACCEPT **"
 $IPTABLES -w -A RULE_17 -j ACCEPT
 #
 # Rule 18 (global)
 #
 echo "Rule 18 (global)"
 #
 $IPTABLES -w -N CHAIN
 $IPTABLES -w -A OUTPUT -p tcp -m tcp --dport 21 -m conntrack --ctstate NEW -j CHAIN
 $IPTABLES -w -N RULE_18
 $IPTABLES -w -A CHAIN -d 22.22.22.22 -j RULE_18
 $IPTABLES -w -A CHAIN -d 22.22.23.23 -j RULE_18
 $IPTABLES -w -A CHAIN -d 22.22.25.50 -j RULE_18
 $IPTABLES -w -A CHAIN -d 192.168.1.1 -j RULE_18
 $IPTABLES -w -A CHAIN -d 192.168.2.1 -j RULE_18
 $IPTABLES -w -A CHAIN -d 192.168.2.40 -j RULE_18
 $IPTABLES -w -A INPUT -p tcp -m tcp --dport 21 -m conntrack --ctstate NEW -j RULE_18
 $IPTABLES -w -A RULE_18 -j LOG --log-level debug --log-prefix "RULE 18 - ACCEPT **"
 $IPTABLES -w -A RULE_18 -j ACCEPT
 #
 # Rule 19 (global)
 #
 echo "Rule 19 (global)"
 #
 # 'catch all' rule
 $IPTABLES -w -N RULE_19
 $IPTABLES -w -A OUTPUT -j RULE_19
 $IPTABLES -w -A INPUT -j RULE_19
 $IPTABLES -w -A FORWARD -j RULE_19
 $IPTABLES -w -A RULE_19 -j LOG --log-level debug --log-prefix "RULE 19 - DENY **"
 $IPTABLES -w -A RULE_19 -j DROP
}

ip_forward() {
 :
 echo 1 > /proc/sys/net/ipv4/ip_forward
}

reset_all() {
 :
 reset_iptables_v4
}

block_action() {
 reset_all
}

stop_action() {
 reset_all
 $IPTABLES -w -P OUTPUT ACCEPT
 $IPTABLES -w -P INPUT ACCEPT
 $IPTABLES -w -P FORWARD ACCEPT
}

check_iptables() {
 IP_TABLES="$1"
 [ ! -e $IP_TABLES ] && return 151
 NF_TABLES=$(cat $IP_TABLES 2>/dev/null)
 [ -z "$NF_TABLES" ] && return 152
 return 0
}
status_action() {
 check_iptables "/proc/net/ip_tables_names"
 ret_ipv4=$?
 check_iptables "/proc/net/ip6_tables_names"
 ret_ipv6=$?
 [ $ret_ipv4 -eq 0 -o $ret_ipv6 -eq 0 ] && return 0
 [ $ret_ipv4 -eq 151 -o $ret_ipv6 -eq 151 ] && {
 echo "iptables modules are not loaded"
 }
 [ $ret_ipv4 -eq 152 -o $ret_ipv6 -eq 152 ] && {
 echo "Firewall is not configured"
 }
 exit 3
}

# See how we were called.
# For backwards compatibility missing argument is equivalent to 'start'

cmd=$1
test -z "$cmd" && {
 cmd="start"
}

case "$cmd" in
 start)
 log "Activating firewall script generated TIMESTAMP"
 check_tools

 check_run_time_address_table_files

 load_modules "nat "
 configure_interfaces
 verify_interfaces

 reset_all
 prolog_commands
 script_body
 ip_forward

 epilog_commands
 RETVAL=$?
 ;;

 stop)
 stop_action
 RETVAL=$?
 ;;

 status)
 status_action
 RETVAL=$?
 ;;

 block)
 block_action
 RETVAL=$?
 ;;

 reload)
 $0 stop
 $0 start
 RETVAL=$?
 ;;

 interfaces)
 configure_interfaces
 RETVAL=$?
 ;;

 test_interfaces)
 FWBDEBUG="echo"
 configure_interfaces
 RETVAL=$?
 ;;

 *)
 echo "Usage $0 [start|stop|status|block|reload|interfaces|test_interfaces]"
 ;;

esac

exit $RETVAL
