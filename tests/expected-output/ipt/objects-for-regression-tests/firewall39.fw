 IFS=$OIFS
 echo $2
 done
}

diff_intf() {
 func=$1
 list1=$2
 list2=$3
 cmd=$4
 for intf in $list1
 do
 echo $list2 | grep -q $intf || {
 # $vlan is absent in list 2
 $func $intf $cmd
 }
 done
}

