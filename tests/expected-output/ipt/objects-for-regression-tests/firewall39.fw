 $IPTABLES -t mangle -A CHAIN -j RULE_9
 $IPTABLES -N CHAIN -t mangle
 $IPTABLES -t mangle -A RULE_9 -j CHAIN
 $IPTABLES -t mangle -A CHAIN -j LOG --log-level info --log-prefix "RULE 9 -- BRANCH "
 $IPTABLES -N CHAIN -t mangle
 $IPTABLES -t mangle -A RULE_9 -j CHAIN
 $IPTABLES -t mangle -A CHAIN -j RETURN
 $IPTABLES -t mangle -A CHAIN
 #
 # Rule 10 (eth1)
 #
 echo "Rule 10 (eth1)"
 #
 $IPTABLES -t mangle -A INPUT -i eth1
 $IPTABLES -t mangle -A PREROUTING -i eth1
 #
 # Rule 11 (eth1)
 #
 echo "Rule 11 (eth1)"
 #
 $IPTABLES -t mangle -A OUTPUT -o eth1
 $IPTABLES -t mangle -A POSTROUTING -o eth1
 #
 # Rule 12 (global)
 #
 echo "Rule 12 (global)"
 #
 $IPTABLES -t mangle -A OUTPUT -p tcp -m tcp -d 192.168.2.10 --dport 80
 $IPTABLES -t mangle -A FORWARD -p tcp -m tcp -d 192.168.2.10 --dport 80
 #
 # Rule 13 (global)
